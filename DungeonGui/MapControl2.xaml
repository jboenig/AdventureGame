<UserControl x:Class="AdventureGameGui.MapControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AdventureGameGui"
             Focusable="True"
             IsTabStop="True"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="Yellow">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <WindowsFormsHost Name="miniMapWinFormHost" Grid.Row="0" HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="300" Background="Beige"/>
    </Grid>
</UserControl>
